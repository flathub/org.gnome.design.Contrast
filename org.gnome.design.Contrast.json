{
    "id": "org.gnome.design.Contrast",
    "runtime": "org.gnome.Platform",
    "runtime-version": "47",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "contrast",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--metadata=X-DConf=migrate-path=/org/gnome/design/Contrast/"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin"
    },
    "modules": [
        {
            "name": "contrast",
            "buildsystem": "meson",
            "sources": [{
                "type": "archive",
                "url": "https://gitlab.gnome.org/-/project/8128/uploads/06d276a9bf45f81a548c4dcefb27437d/contrast-0.0.11.tar.xz",
                "sha256": "b217c4433be6e6f97e20f63a27dcdcfe4098a0ba083c036772ba6cd2eb4569a6"
            }]
        }
    ]
}
